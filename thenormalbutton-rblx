local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
local Window = Library:NewWindow("The Normal Button")
local Tab = Window:NewSection("Main")

Tab:CreateButton("Autofarm TNB", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/wh0asked1/thenormalbutton-rblx/refs/heads/main/TNButton-GUI",true))()

end)
local Tab = Window:NewSection("Minigame BETA")
Tab:CreateButton("AW Press The Button!", function()
-- Alternative: Fire all at once without delays
local buttonsFolder = workspace["Press the Buttons!"]

for _, child in ipairs(buttonsFolder:GetChildren()) do
    local real = child:FindFirstChild("real")
    if real then
        local clickDetector = real:FindFirstChildOfClass("ClickDetector")
        if clickDetector then
            fireclickdetector(clickDetector)
        end
    end
end

print("Fired all ClickDetectors!")

end)
Tab:CreateButton("AW Obby Race", function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace["\226\154\148\239\184\143 Obby Race"].win.CFrame

end)
